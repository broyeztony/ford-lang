contract Events;

let myAddress = address("0xfe3091F63A0b0b1cf81ff53102434aa287aC5289");

def receive(amount) {

    let ev = { address: msg.sender, amount: amount };

    if send(myAddress, amount) {
        ev.message = "Received some ether";
        emit("Received", ev);
    } else {
        ev.message = "Transfer failure";
        emit("TransferError", ev);
    }
}

