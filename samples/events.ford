contract Events;

let myAddress = address("0xfe3091F63A0b0b1cf81ff53102434aa287aC5289");

def receive(amount) {

    let ev = event({
        address: msg.sender(),
        message: "",
        amount,
    });

    if send(myAddress, amount) {
        ev.data.message = "Received some ether";
        emit("Received", ev);
    } else {
        ev.message = "Houston, we got a problem";
        emit("TransferError", ev);
    }

}

